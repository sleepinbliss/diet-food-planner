{
  "name": "diet-food-planner",
  "version": "1.0.0",
  "description": "A full-stack diet and food planning application",
  "main": "index.js",
  "scripts": {
    "start": "node src/backend/app.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
